{
  "name": "aerophant/ramda",
  "description": "Functional Programming Library in PHP",
  "keywords": ["functional", "utility", "auto-curry", "php"],
  "homepage": "https://github.com/aerophant/ramda",
  "license": "MIT",
  "authors": [
    {
      "name": "Siwapun Siwaporn",
      "email": "siwapun.si@gmail.com"
    }
  ],
  "require": {
    "php": "~7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.1",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "Aerophant\\Ramda\\": "src"
    },
    "files": [
      "src/function.php",
      "src/list.php",
      "src/logic.php",
      "src/math.php",
      "src/relation.php",
      "src/type.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Aerophant\\RamdaTest\\" : "test"
    }
  },
  "scripts": {
    "test": "phpunit"
  }

}
